/*
Problem: Create a Car class with attributes like brand and model. Then create
an instance of this class.
*/

class Car:
    # self = je call korbe.
    def __init__(self, brand, model):
        self.brand = brand
        self.model = model

car = Car("Toyota", "Corolla")
print(car.brand)
print(car.model)

new_car = Car("Tata", "Safari")
print(new_car.model)
